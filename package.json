{
  "name": "@bentoapp/types",
  "version": "1.1.0",
  "description": "Common types used by Bento JavaScript SDKs",
  "author": "Bento",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "types": "build/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx --quiet && prettier --write .",
    "checks": "eslint . --ext .js,.jsx,.ts,.tsx --quiet && prettier --check .",
    "build:types": "tsc -p tsconfig.types.json",
    "build:types:watch": "tsc -p tsconfig.types.json --watch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trybento/types.git"
  },
  "bugs": {
    "url": "https://github.com/trybento/types/issues"
  },
  "homepage": "https://github.com/trybento/types#readme"
}